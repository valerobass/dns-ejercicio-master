;
;Reverse zone configuration for  57.168.192.in-addr.arpa
;

$TTL	86400
$ORIGIN sistema.test.

@	IN	SOA tierra.sistema.test.  root.sistema.test.(
		2024102002 		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			  7200   	; Negative Cache TTL
)

; Name servers
@					IN	NS	tierra.sistema.test.
@					IN	NS	venus.sistema.test.



;   PTR records (reverse lookups)
101     IN  mercurio.sistema.test.
102     IN  venus.sistema.test.
103     IN  tierra.sistema.test.
104     IN  marte.sistema.test.
